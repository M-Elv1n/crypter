{
  "extends": "standard",
  "plugins": [
    "standard",
    "promise",
    "import"
  ],
  "globals": {
    "XMLHttpRequest": true,
    "it": true,
    "describe": true,
  },
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "never",
      "imports": "never",
      "exports": "never",
      "functions": "never"
    }],
    "no-console": ["error", {
      "allow": ["warn", "error", "info"]
    }],
  }
}
